{
    "name": "L&C-api",
    "version": "1.0.0",
    "description": "E-commerce API built with Node.js and Express - for Luxury & Cheap.",
    "builds":[
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
    
}